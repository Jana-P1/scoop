  <% if (!user) { %>
    <link rel="stylesheet" href="/stylesheets/siwg.css">
  <% } %>
</head>
<body>
  <nav>
    <div id="icon">
      <i class="fa-solid fa-ice-cream fa-2x"></i>
    </div>
    <div id="title">
      <h1>Scoop</h1>
    </div>
    <div id="links">
      <a href="/shops" <%- title === "All Shops" ? 'class="active"' : "" %> id="shops">All Shops</a>
      <a href="/profiles" <%- title === "All Profiles" ? 'class="active"' :'' %> id="profiles"> All Profiles</a>
    </div>
    <div>
      <% if (user) { %>
        <a href="/auth/logout" id="logout">
          <button type="submit">Logout</button>
        </a>
      <% } else { %>
        <a href="/auth/google" class="siwg-link">
          <button type="submit" class="siwg">
            <img id="siwg-image" alt="Sign in with Google" />
          </button>
        </a>
      <% } %>
    </div>
  </nav>
