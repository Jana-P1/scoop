  <% if (!user) { %>
    <link rel="stylesheet" href="/stylesheets/siwg.css">
  <% } %>
</head>
<body>
  <nav>
    <i class="fa-solid fa-ice-cream fa-2x"></i>
    <h1>Scoop</h1>
    <a href="/shops" <%- title === "All Shops" ? 'class="active"' : "" %>>Ice Cream Shops</a>
    <a href="/profiles" <%- title === "All Profiles" ? 'class="active"' :'' %>> Ice Cream Lovers</a>
    <div>
      <% if (user) { %>
        <a href="/auth/logout">
          <button type="submit">Logout</button>
        </a>
      <% } else { %>
        <a href="/auth/google" class="siwg-link">
          <button type="submit" class="siwg">
            <img id="siwg-image" alt="Sign in with Google" />
          </button>
        </a>
      <% } %>
    </div>
  </nav>
